{
  "manifest_version": 2,
  "name": "MEETboard",
  "icons": {
    "16": "icons/board-16.png",
    "32": "icons/board-40.png",
    "48": "icons/board-53.png",
    "128": "icons/board-160.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://meet.google.com/*"
      ],
      "js": [
        "js/content-script.js"
      ]
    }
  ],
  "version": "0.1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}